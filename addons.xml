<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<addons>
   <addon id='repository.beam.xbmc-addons' name='Beam Kodi Add-ons' version='1.0.3' provider-name='Beam'>
      <extension point='xbmc.addon.repository' name='Beam Kodi Add-ons Repository'>
         <info compressed='false'>https://xbmc-repo.bimovi.cz/addons.xml</info>
         <checksum>https://xbmc-repo.bimovi.cz/addons.xml.md5</checksum>
         <datadir zip='true'>https://xbmc-repo.bimovi.cz/packages/</datadir>
      </extension>
      <extension point='xbmc.addon.metadata'>
         <summary>Beam Kodi Add-ons Repository</summary>
         <description>Add-ons created by Andrew Beam</description>
         <platform>all</platform>
         <source>https://github.com/beam/repository.beam.xbmc-addons</source>
         <email>ondra@bimovi.cz</email>
      </extension>
   </addon>
   <addon id='service.subtitles.titulky.com' name='Titulky.com' version='1.4.2' provider-name='Beam'>
      <requires>
         <import addon='xbmc.python' version='2.14.0'/>
         <import addon='script.module.simplejson' version='2.0.10'/>
      </requires>
      <extension point='xbmc.subtitle.module' library='service.py'/>
      <extension point='xbmc.addon.metadata'>
         <summary>Titulky.com</summary>
         <description lang='en'>Search and download subtitles from titulky.com (Czech/Slovak only)</description>
         <description lang='cs'>Vyhledávání a stahování titulků z titulky.com</description>
         <description lang='sk'>Vyhľadávanie a sťahovanie titulkov z titulky.com</description>
         <source>https://github.com/beam/service.subtitles.titulky.com</source>
         <email>ondra@bimovi.cz</email>
      </extension>
   </addon>
</addons>